            app:layout_constraintStart_toStartOf="@+id/hello_ken"
            app:layout_constraintTop_toBottomOf="@+id/hello_ken" />

        <TextView
            android:id="@+id/hello_ken"
            android:layout_width="132.43dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:gravity="top"
            android:lineSpacingExtra="11sp"
            android:text="Hello Ken"
            android:textAppearance="@style/hello_ken"
            android:translationY="-5.62sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageViewbob"
            app:layout_constraintStart_toEndOf="@+id/imageViewbob"
            app:layout_constraintTop_toTopOf="@+id/imageViewbob" />

        <ImageView
            android:id="@+id/imageViewbob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="122dp"
            android:src="@drawable/face"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/enable_dark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="32dp"
            android:gravity="top"
            android:lineSpacingExtra="-3sp"
            android:text="Enable dark Mode"
            android:textAppearance="@style/enable_dark"
            app:layout_constraintStart_toStartOf="@+id/imageViewbob"
            app:layout_constraintTop_toBottomOf="@+id/imageViewbob" />

        <RelativeLayout
            android:id="@+id/edit_prof"
            android:layout_width="342dp"
            android:layout_height="62dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="290dp"
            android:background="#FFFFFF"
            android:elevation="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/notif_set"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
