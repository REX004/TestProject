            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="3.52dp"
                android:layout_height="7.06dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="27dp"
                android:layout_marginEnd="27dp"
                android:layout_marginBottom="29dp"
                android:src="@drawable/next_znak" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/referals"
            android:layout_width="342dp"
            android:layout_height="62dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="12dp"
            android:background="#FFFFFF"
            android:elevation="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_banks"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/referals1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="9dp"
                android:layout_marginTop="13dp"
                android:layout_toEndOf="@+id/image_5"
                android:gravity="top"
                android:lineSpacingExtra="-3sp"
                android:text="Referrals"
                android:textAppearance="@style/edit_profil"
                tools:ignore="DuplicateIds" />

            <ImageView
                android:id="@+id/image_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="13dp"
                android:layout_marginTop="18dp"
                android:layout_marginBottom="25dp"
                android:src="@drawable/statements" />

            <TextView
                android:id="@+id/check_no"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_below="@+id/referals1"
                android:layout_marginStart="9dp"
                android:layout_marginTop="4dp"
                android:layout_toEndOf="@+id/image_5"
                android:gravity="top"
                android:lineSpacingExtra="2sp"
                android:text="check no of friends and earn"
                android:textAppearance="@style/name_phone_"
                android:translationY="-0.97sp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="3.52dp"
                android:layout_height="7.06dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="27dp"
                android:layout_marginEnd="27dp"
                android:layout_marginBottom="29dp"
                android:src="@drawable/next_znak" />

        </RelativeLayout>

        <RelativeLayout
