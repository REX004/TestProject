            <TextView
                android:id="@+id/statments"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="9dp"
                android:layout_marginTop="13dp"
                android:layout_toEndOf="@+id/image_2"
                android:gravity="top"
                android:lineSpacingExtra="-3sp"
                android:text="Statements &amp; Reports"
                android:textAppearance="@style/edit_profil" />

            <ImageView
                android:id="@+id/image_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="13dp"
                android:layout_marginTop="18dp"
                android:layout_marginBottom="25dp"
                android:src="@drawable/statements" />

            <TextView
                android:id="@+id/mute_umute"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_below="@+id/statments"
                android:layout_marginStart="9dp"
                android:layout_marginTop="4dp"
                android:layout_toEndOf="@+id/image_2"
                android:gravity="top"
                android:lineSpacingExtra="2sp"
